{
  "name": "spekkionu/passworddb",
  "description": "Password Database",
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/lumen-framework": "5.1.*",
    "vlucas/phpdotenv": "~2.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "fzaninotto/faker": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  }
}
